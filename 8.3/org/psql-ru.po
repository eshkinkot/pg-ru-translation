#: help.c:89
msgid "Usage:"
msgstr "Использование:"

#: help.c:90
msgid "  psql [OPTIONS]... [DBNAME [USERNAME]]\n"
msgstr "  psql [ОПЦИИ]... [DBNAME [USERNAME]]\n"

#: help.c:92
msgid "General options:"
msgstr "Опции общего назначения:"

#: help.c:97
#, c-format
msgid ""
"  -d DBNAME       specify database name to connect to (default: \"%s\")\n"
msgstr ""
"  -d DBNAME        имя базы данных для подсоединения (по-умолчанию: \"%s\")\n"

#: help.c:98
msgid "  -c COMMAND      run only single command (SQL or internal) and exit"
msgstr ""
"  -c COMMAND      выполнить только одну команду (SQL или внутреннюю) и выйти"

#: help.c:99
msgid "  -f FILENAME     execute commands from file, then exit"
msgstr "  -f FILENAME     выполнить команды из файла и выйти"

#: help.c:100
msgid "  -1 (\"one\")      execute command file as a single transaction"
msgstr ""

#: help.c:101
msgid "  -l              list available databases, then exit"
msgstr "  -l              вывести список баз данных и выйти"

#: help.c:102
msgid "  -v NAME=VALUE   set psql variable NAME to VALUE"
msgstr ""
"  -v NAME=VALUE   установить переменную psql с именем 'NAME' в значение "
"'VALUE'"

#: help.c:103
msgid "  -X              do not read startup file (~/.psqlrc)"
msgstr "  -X              не читать файл настроек при старте (~/.psqlrc)"

#: help.c:104
msgid "  --help          show this help, then exit"
msgstr "  --help          показать эту подсказку и выйти"

#: help.c:105
msgid "  --version       output version information, then exit"
msgstr "  --version       показать информацию о версии и выйти"

#: help.c:107
msgid ""
"\n"
"Input and output options:"
msgstr ""
"\n"
"Опции ввода/вывода:"

#: help.c:108
msgid "  -a              echo all input from script"
msgstr "  -a             отображать все прочитанные строки"

#: help.c:109
msgid "  -e              echo commands sent to server"
msgstr "  -e             отображать команды отправленные серверу"

#: help.c:110
msgid "  -E              display queries that internal commands generate"
msgstr ""
"  -E              показывать запросы, порожденные внутренними  командами"

#: help.c:111
msgid "  -q              run quietly (no messages, only query output)"
msgstr ""
"  -q             показывать только результаты запросов, никаких сообщений"

#: help.c:112
msgid "  -o FILENAME     send query results to file (or |pipe)"
msgstr "  -o FILENAME     направить результаты запроса в файл (или |канал)"

#: help.c:113
msgid "  -n              disable enhanced command line editing (readline)"
msgstr ""
"  -n              отключить расширенный редактор командной строки (readline)"

#: help.c:114
msgid "  -s              single-step mode (confirm each query)"
msgstr "  -s              пошаговый режим (подтверждение каждого запроса)"

#: help.c:115
msgid "  -S              single-line mode (end of line terminates SQL command)"
msgstr ""
"  -S              однострочный режим (конец строки завершает команду SQL)"

#: help.c:116
#, fuzzy
msgid "  -L FILENAME     send session log to file"
msgstr "  -o FILENAME     направить результаты запроса в файл (или |канал)"

